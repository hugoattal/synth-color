{
    "name": "synth-color",
    "version": "1.0.3",
    "author": {
        "name": "Hugo Attal",
        "url": "https://twitter.com/HugoAttal"
    },
    "description": "Library for managing and manipulating colors in different color spaces and models.",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "license": "ISC",
    "scripts": {
        "build": "rimraf dist && vite build",
        "dev": "vite build --watch",
        "test": "vitest",
        "dev:test": "vitest --ui"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.umd.js",
            "import": "./dist/index.mjs"
        },
        "./*": "./*"
    },
    "files": [
        "dist/**"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/hugoattal/synth-color.git"
    },
    "homepage": "https://github.com/hugoattal/synth-color#readme",
    "url": "https://github.com/hugoattal/synth-color",
    "bugs": {
        "url": "https://github.com/hugoattal/synth-color/issues"
    },
    "devDependencies": {
        "@types/node": "^18.11.9",
        "@vitest/ui": "^0.25.8",
        "eslint": "^8.28.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-sort-keys-custom-order": "^1.0.5",
        "rimraf": "^3.0.2",
        "vite": "^3.2.4",
        "vite-plugin-dts": "^1.7.1",
        "vitest": "^0.25.3"
    }
}
